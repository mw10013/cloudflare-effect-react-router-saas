{
  "extends": "../../tsconfig.base.jsonc",
  "compilerOptions": {
    "target": "esnext",
    "lib": ["ESNext", "DOM"],
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "preserveWatchOutput": true
  },
  "exclude": ["node_modules"]
}
