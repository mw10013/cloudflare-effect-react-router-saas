{
  "include": ["**/*.ts", "../worker-configuration.d.ts"],
  "compilerOptions": {
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "checkJs": false,
    "types": ["vitest", "@cloudflare/vitest-pool-workers"],
    "lib": ["es2022"],
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "paths": {
      "~/*": ["../app/*"]
    }
  }
}
